{
    "name": "rekryt/ratchet-revolt-example",
    "description": "Example of use new PHP 8.1 Fibers",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Krupkin Sergey",
            "email": "rekrytkw@gmail.com",
            "homepage": "https://vk.com/krupkin.sergey"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "rekryt/react-adapter",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/Rekryt/react-adapter.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "php": "^8.1",
        "revolt/event-loop": "^0.2.1",
        "amphp/amp": "v3.x-dev as v2.6.1",
        "rekryt/react-adapter": "1.0",
        "react/http": "^1.6",
        "cboden/ratchet": "^0.4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "RatchetRevoltExample\\": "src/",
            "Amp\\ReactAdapter\\": "vendor/rekryt/react-adapter/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RatchetRevoltExample\\": "tests/RatchetRevoltExample"
        }
    }
}
